{
    "name": "sdailover/yii2-sdailoverwidgets",
    "description": "The SDaiLover Widget packages for the Yii 2.0 Framework",
    "keywords": [
        "sdailover",
        "yii2",
        "yii2-widget",
        "yii2-extension",
        "sdailover-widget"
    ],
    "type": "yii2-widget",
    "license": "BSD-3-Clause",
    "support": {
        "homepage": "https://www.sdailover.com/",
        "issues": "https://github.com/sdailover/yii2-sdailoverwidgets/issues",
        "source": "https://github.com/sdailover/yii2-sdailoverwidgets"
    },
    "authors": [
        {
            "name": "Stephanus Bagus Saputra ( 戴 Dai 偉 Wie 峯 Funk )",
            "email": "wiefunk@stephanusdai.web.id",
            "homepage": "https://stephanusdai.web.id/"
        }
    ],
    "funding": [
        {
            "type": "patreon",
            "url": "https://www.patreon.com/sdailover"
        },
        {
            "type": "github",
            "url": "https://github.com/sponsors/sdailover"
        },
        {
            "type": "opencollective",
            "url": "https://opencollective.com/sdailover"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0",
        "ext-json": "*",
        "yiisoft/yii2": "^2.0.42",
        "yiisoft/yii2-bootstrap5": "~2.0.4",
        "twbs/bootstrap-icons": "^1.11.3",
        "bower-asset/bootstrap": "^5.3.3",
        "bower-asset/jquery": "^3.7.1"
    },
    "require-dev": {
        "cweagans/composer-patches": "^1.7",
        "yiisoft/yii2-coding-standards": "~2.0"
    },
    "config": {
        "allow-plugins": {
            "cweagans/composer-patches": true,
            "yiisoft/yii2-composer": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "sdailover\\yii\\widgets\\": "src"
        }
    }
}